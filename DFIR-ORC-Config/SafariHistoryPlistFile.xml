<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="SafariHistoryPlistFile">
        <ntfs_find path_match="%%users.homedir%%\Library\Safari\History.plist"/>
    </sample>
    <sample name="SafariHistoryPlistFile">
        <ntfs_find path_match="Users\*\AppData\Local\Apple Computer\Safari\History.plist"/>
    </sample>
    <sample name="SafariHistoryPlistFile">
        <ntfs_find path_match="Users\*\AppData\Roaming\Apple Computer\Safari\History.plist"/>
    </sample>
</getthis>
