    <sample name="WindowsSystemRegistryFilesBackup">
        <ntfs_find path_match="Windows\System32\config\RegBack\SAM"/>
    </sample>
    <sample name="WindowsSystemRegistryFilesBackup">
        <ntfs_find path_match="Windows\System32\config\RegBack\SECURITY"/>
    </sample>
    <sample name="WindowsSystemRegistryFilesBackup">
        <ntfs_find path_match="Windows\System32\config\RegBack\SOFTWARE"/>
    </sample>
    <sample name="WindowsSystemRegistryFilesBackup">
        <ntfs_find path_match="Windows\System32\config\RegBack\SYSTEM"/>
    </sample>
</getthis>
