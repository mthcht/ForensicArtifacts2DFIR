    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="%%users.homedir%%\.cshrc"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="etc\csh.cshrc"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="etc\csh.login"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="etc\csh.logout"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="private\etc\csh.cshrc"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="private\etc\csh.login"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="private\etc\csh.logout"/>
    </sample>
    <sample name="CShellConfigurationFile">
        <ntfs_find path_match="Users\*\AppData\Local\Packages\*\LocalState\rootfs\home\*\.cshrc"/>
