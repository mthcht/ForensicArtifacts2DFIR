<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="ChromePlatformNotifications">
        <ntfs_find path_match="%%users.homedir%%\Library\Application Support\Google\Chrome\*\Platform Notifications\*"/>
    </sample>
    <sample name="ChromePlatformNotifications">
        <ntfs_find path_match="%%users.homedir%%\.config\google-chrome\*\Platform Notifications\*"/>
    </sample>
    <sample name="ChromePlatformNotifications">
        <ntfs_find path_match="Users\*\AppData\Local\Google\Chrome\User Data\*\Platform Notifications\*"/>
    </sample>
</getthis>
