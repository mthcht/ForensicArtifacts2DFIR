<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="KornShellConfigurationFile">
        <ntfs_find path_match="%%users.homedir%%\.ksh"/>
    </sample>
    <sample name="KornShellConfigurationFile">
        <ntfs_find path_match="etc\kshrc"/>
    </sample>
    <sample name="KornShellConfigurationFile">
        <ntfs_find path_match="private\etc\kshrc"/>
    </sample>
    <sample name="KornShellConfigurationFile">
        <ntfs_find path_match="Users\*\AppData\Local\Packages\*\LocalState\rootfs\home\*\.ksh"/>
    </sample>
</getthis>
