<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="MicrosoftSqlServerErrorLogs">
        <ntfs_find path_match="Program Files\Microsoft SQL Server\*\MSSQL\LOG\ERRORLOG*"/>
    </sample>
    <sample name="MicrosoftSqlServerErrorLogs">
        <ntfs_find path_match="Program Files (x86)\Microsoft SQL Server\*\MSSQL\LOG\ERRORLOG*"/>
    </sample>
</getthis>
