    <sample name="WindowsSystemRegistryFiles">
        <ntfs_find path_match="System Volume Information\Syscache.hve"/>
    </sample>
    <sample name="WindowsSystemRegistryFiles">
        <ntfs_find path_match="Windows\System32\config\SAM"/>
    </sample>
    <sample name="WindowsSystemRegistryFiles">
        <ntfs_find path_match="Windows\System32\config\SECURITY"/>
    </sample>
    <sample name="WindowsSystemRegistryFiles">
        <ntfs_find path_match="Windows\System32\config\SOFTWARE"/>
    </sample>
    <sample name="WindowsSystemRegistryFiles">
        <ntfs_find path_match="Windows\System32\config\SYSTEM"/>
    </sample>
</getthis>
