<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="ShellProfileFile">
        <ntfs_find path_match="%%users.homedir%%\.profile"/>
    </sample>
    <sample name="ShellProfileFile">
        <ntfs_find path_match="etc\profile"/>
    </sample>
    <sample name="ShellProfileFile">
        <ntfs_find path_match="private\etc\profile"/>
    </sample>
    <sample name="ShellProfileFile">
        <ntfs_find path_match="Users\*\AppData\Local\Packages\*\LocalState\rootfs\home\*\.profile"/>
    </sample>
</getthis>
