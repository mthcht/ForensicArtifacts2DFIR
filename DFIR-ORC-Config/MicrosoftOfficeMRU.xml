<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="MicrosoftOfficeMRU">
        <ntfs_find path_match="%%users.homedir%%\Library\Preferences\com.microsoft.office.plist"/>
    </sample>
    <sample name="MicrosoftOfficeMRU">
        <ntfs_find path_match="%%users.homedir%%\Library\Containers\com.microsoft.*\Data\Library\Preferences\com.microsoft.*.securebookmarks.plist"/>
    </sample>
    <sample name="MicrosoftOfficeMRU">
        <reg_find hive="HKEY_USERS" key="%%users.sid%%\Software\Microsoft\Office\*\*\File MRU" value="Item *"/>
    </sample>
    <sample name="MicrosoftOfficeMRU">
        <reg_find hive="HKEY_USERS" key="%%users.sid%%\Software\Microsoft\Office\*\*\Place MRU" value="Item *"/>
    </sample>
</getthis>
