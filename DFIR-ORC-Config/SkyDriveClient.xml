<?xml version="1.0" ?>
<getthis reportall="">
    <output compression="fast"/>
    <location>%SystemDrive%\</location>
    <sample name="SkyDriveClient">
        <ntfs_find path_match="Users\*\AppData\Local\Microsoft\SkyDrive\logs\*.log"/>
    </sample>
    <sample name="SkyDriveClient">
        <ntfs_find path_match="Users\*\AppData\Local\Microsoft\SkyDrive\setup\logs\*.log"/>
    </sample>
    <sample name="SkyDriveClient">
        <ntfs_find path_match="Users\*\AppData\Local\Microsoft\SkyDrive\settings\ApplicationSettings.xml"/>
    </sample>
    <sample name="SkyDriveClient">
        <ntfs_find path_match="Users\*\AppData\Local\Microsoft\SkyDrive\settings\*.dat"/>
    </sample>
    <sample name="SkyDriveClient">
        <ntfs_find path_match="Users\*\AppData\Local\Microsoft\SkyDrive\settings\*.ini"/>
    </sample>
</getthis>
